{
  "name": "@interslavic/razumlivost-core",
  "version": "1.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "lint": "eslint . --fix",
    "lint:ci": "eslint .",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/medzuslovjansky/razumlivost.git",
    "directory": "packages/core"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/medzuslovjansky/razumlivost/issues"
  },
  "homepage": "https://github.com/medzuslovjansky/razumlivost#readme",
  "engines": {
    "node": ">=16"
  },
  "devDependencies": {
    "@interslavic/eslint-config-razumlivost": "^1.0.0",
    "@interslavic/prettier-config-razumlivost": "^1.0.0",
    "@interslavic/typescript-config-razumlivost": "^1.0.0",
    "@types/lodash": "^4.14.182",
    "ts-node": "^10.9.1",
    "typescript": "~4.3.5",
    "zora": "^5.1.0",
    "zx": "^7.0.0"
  },
  "dependencies": {
    "@interslavic/steen-utils": "^1.6.0",
    "dictionary-be": "^1.0.0",
    "dictionary-bg": "^1.4.0",
    "dictionary-cs": "^3.1.0",
    "dictionary-hr": "^1.6.0",
    "dictionary-mk": "^1.1.0",
    "dictionary-pl": "^1.3.0",
    "dictionary-ru": "^2.1.0",
    "dictionary-sk": "^1.4.0",
    "dictionary-sl": "^2.1.0",
    "dictionary-sr": "^3.1.0",
    "dictionary-uk": "^3.1.0",
    "lodash": "^4.17.21",
    "nodehun": "^3.0.2",
    "tslib": "^2.4.0"
  },
  "prettier": "@interslavic/prettier-config-razumlivost",
  "eslintConfig": {
    "extends": "@interslavic/razumlivost",
    "rules": {
      "unicorn/no-null": "off",
      "unicorn/no-array-callback-reference": "off",
      "unicorn/no-array-reduce": "off",
      "unicorn/prevent-abbreviations": "off",
      "unicorn/consistent-function-scoping": "off",
      "@typescript-eslint/consistent-type-imports": "error",
      "@typescript-eslint/no-explicit-any": "off",
      "@typescript-eslint/no-unused-vars": [
        "error",
        {
          "argsIgnorePattern": "^_"
        }
      ]
    }
  }
}
